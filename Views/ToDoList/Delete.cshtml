@model ToDoListModel
@{
    ViewData["Title"] = "Delete this Item";
}
<div>
    <form asp-action="Delete">
        <div class="form-group">
            <label>Title</label>
            <input asp-for="Title" class="form-control" />
            <br />
        </div>
        <div class="form-group">
            <label>Status</label>
            <input asp-for="isChecked" class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Delete?" class="btn btn-success float-right" />
        </div>
    </form>
</div>